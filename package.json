{
  "name": "triage-ai",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered patient triage system for healthcare",
  "author": "Pranjal Gupta <pranjal.gupta@dataxlr8.ai>",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report test-results/html-report",
    "lint": "turbo run lint",
    "db:migrate": "npm run migrate --workspace=apps/api",
    "db:seed": "npm run seed --workspace=apps/api"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@vercel/node": "^3.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "npm@10.0.0",
  "dependencies": {
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.1"
  }
}
