# TriageAI Environment Variables
# Copy this to .env and fill in your values

# LLM API Keys (choose one - Gemini uses Google Cloud credits)
# Option 1: Gemini (Google Cloud) - uses your GCP credits
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash

# Option 2: Groq (free tier - get key at console.groq.com)
GROQ_API_KEY=your_groq_api_key_here

# Option 3: Ollama (local - no key needed, runs on your machine)
OLLAMA_URL=http://127.0.0.1:11434
OLLAMA_MODEL=llama3.2

# Database (PostgreSQL or Supabase)
DATABASE_URL=postgresql://user:password@localhost:5432/triageai

# Supabase (for conversation persistence)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here

# Redis
REDIS_URL=redis://localhost:6379

# Auth (Auth0 or Cognito)
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_CLIENT_ID=your_client_id
AUTH0_CLIENT_SECRET=your_client_secret

# AWS (for production)
AWS_REGION=ap-southeast-2
AWS_ACCESS_KEY_ID=your_aws_key
AWS_SECRET_ACCESS_KEY=your_aws_secret

# Application
NODE_ENV=development
PORT=3001
